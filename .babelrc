{
  "presets": ["backpack-core/babel", "@babel/preset-env"],
  "plugins": [
    "@babel/plugin-proposal-do-expressions",
    [
      "babel-plugin-root-import",
      {
        "rootPathPrefix": "~",
        "rootPathSuffix": "src"
      }
    ]
  ],
  "env": {
    "development": {
      "sourceMaps": "inline",
      "retainLines": true
    }
  }
}
